{{- $format := "20060102T150405Z" -}}
BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//{{ site.Title }}//NONSGML v1.0//EN
X-WR-CALNAME:{{ site.Title }}
NAME:{{ site.Title }}
REFRESH-INTERVAL;VALUE=DURATION:P3H
CALSCALE:GREGORIAN
{{ range (where site.RegularPages "Section" "events") -}}
    {{- $context := dict
      "event" .
      "title" (partial "calendar/artists.ics" .)
      "term" "home"
    -}}
    {{- partial "calendar/event.ics" $context -}}
{{- end -}}
END:VCALENDAR
