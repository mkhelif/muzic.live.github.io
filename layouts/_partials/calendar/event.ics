BEGIN:VEVENT
UID:{{ hash.XxHash .RelPermalink }}
{{ $eventDate := .Params.eventDate | time.AsTime -}}
DTSTAMP:{{ partial "calendar/time.ics" $eventDate }}
DTSTART:{{ partial "calendar/time.ics" $eventDate }}
{{ $artistsCount := printf "%dh" (.Params.artists | len) -}}
{{ $duration := time.ParseDuration $artistsCount -}}
DTEND:{{ partial "calendar/time.ics" ($eventDate.Add $duration) }}
SUMMARY:{{ partial "calendar/artists.ics" . }}
DESCRIPTION:{{ partial "calendar/artists.ics" . }}
LOCATION:{{- partial "calendar/location.ics" . }}
{{- partial "calendar/apple.ics" . }}
END:VEVENT
