BEGIN:VEVENT
UID:{{ hash.XxHash .event.RelPermalink }}
{{ partial "calendar/event-date.ics" . }}
SUMMARY:{{ partial "calendar/summary.ics" (dict "event" .event "term" .term "title" .title) }}
DESCRIPTION:{{ partial "calendar/artists.ics" .event }}
LOCATION:{{ replace (partial "calendar/location.ics" .event) "," "\\," }}
{{ partial "calendar/alarm.ics" . }}
{{ partial "calendar/apple.ics" .event }}
{{ partial "calendar/outlook.ics" . }}
END:VEVENT
